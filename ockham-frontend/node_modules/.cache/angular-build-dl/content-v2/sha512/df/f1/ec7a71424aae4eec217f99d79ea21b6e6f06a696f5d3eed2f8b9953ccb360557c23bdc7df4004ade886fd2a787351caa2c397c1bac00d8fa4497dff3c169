    let ActivityDetailComponent = class ActivityDetailComponent {
      constructor(actr, modalService, activityService, authService, sanitizer) {
        this.actr = actr;
        this.modalService = modalService;
        this.activityService = activityService;
        this.authService = authService;
        this.sanitizer = sanitizer;
        this.isRegistered = false;
        this.registrationClosed = false;
        this._canRegister = true;
      }

      ngOnInit() {
        this.actr.data.subscribe(data => {
          this.activity = data.activity;

          if (this.canRegister(this.activity.registration_opens, this.activity.registration_closes)) {
            this._canRegister = true;
          } else {
            this._canRegister = false;
          }

          console.log(this.activity);
        });

        const member = this.authService._getUser();

        if (member) {
          this.member = member;
          this.getRegistrations();
        }
      }

      get description() {
        return this.sanitizer.bypassSecurityTrustHtml(this.activity.description);
      }

      canRegister(open_date, closing_date) {
        if (new Date(open_date).getTime() <= Date.now() && Date.now() < new Date(closing_date).getTime()) {
          return true;
        }

        return false;
      }

      getRegistrations() {
        this.activityService.getRegistrations(this.activity.id, this.member.id).subscribe(registrations => {
          console.log(registrations);
          this.registration = registrations[0];

          if (registrations.length > 0) {
            this.isRegistered = true;
          }
        }, error => {
          console.log("error", error);
        });
      }

      onRegister() {
        const modalRef = this.modalService.open(src_app_shared_components_activity_helpers_register_modal_register_modal_component__WEBPACK_IMPORTED_MODULE_4__["RegisterModalComponent"], {
          backdrop: 'static'
        });
        modalRef.componentInstance.activityId = this.activity.id;
        modalRef.componentInstance.showSearchbar = false;
        modalRef.componentInstance.selectedMember = this.member;
        modalRef.componentInstance.activityOptions = this.activity.options;
        modalRef.componentInstance.register.subscribe(data => {
          this.registerMember(data);
          modalRef.close();
        });
      }

      onDeRegister() {
        const modalRef = this.modalService.open(src_app_shared_components_delete_modal_delete_modal_component__WEBPACK_IMPORTED_MODULE_7__["DeleteModalComponent"]);
        modalRef.componentInstance.action = "de-register";
        modalRef.componentInstance.objRef = "activity";
        modalRef.componentInstance.delete.subscribe(() => {
          this.deRegister();
        });
      }

      deRegister() {
        this.activityService.deleteRegistration(this.activity.id, this.registration.id).subscribe(response => {
          console.log("You have been de-registerd successfully", response);
          this.isRegistered = false;
        }, error => {
          console.log("An error occurred while de-registering", error);
        });
      }

      registerMember(data) {
        console.log(data);
        this.activityService.registerMember(data).subscribe(registration => {
          this.isRegistered = true;
          this.getRegistrations();
        }, error => {
          console.log("An erro has occurred while registering", error);
        });
      }

    };

    ActivityDetailComponent.ctorParameters = () => [{
      type: _angular_router__WEBPACK_IMPORTED_MODULE_2__["ActivatedRoute"]
